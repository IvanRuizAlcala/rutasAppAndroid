<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/texture"
    tools:context=".datosCentro">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="381dp"
        android:layout_height="709dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="-30dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bgcole" />

    <ImageView
        android:id="@+id/facebook"
        android:layout_width="112dp"
        android:layout_height="94dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="559dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="57dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toEndOf="@+id/web"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/logoface" />

    <ImageView
        android:id="@+id/web"
        android:layout_width="112dp"
        android:layout_height="94dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="559dp"
        android:layout_marginEnd="197dp"
        android:layout_marginBottom="57dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:srcCompat="@drawable/webicon" />

    <ImageButton
        android:id="@+id/atras"
        android:layout_width="151dp"
        android:layout_height="36dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

</androidx.constraintlayout.widget.ConstraintLayout>