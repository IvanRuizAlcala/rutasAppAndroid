<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondodos"
    tools:context=".MainActivity3">

    <ImageView
        android:id="@+id/imageninsti"
        android:layout_width="161dp"
        android:layout_height="181dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="169dp"
        android:layout_marginEnd="75dp"
        android:layout_marginBottom="289dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.641"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/rutas"
        android:layout_width="136dp"
        android:layout_height="134dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageninsti"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.418"
        app:srcCompat="@drawable/rutas" />

    <ImageView
        android:id="@+id/centro"
        android:layout_width="180dp"
        android:layout_height="128dp"
        android:layout_marginStart="115dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toTopOf="@+id/imageninsti"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/centro" />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="81dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="#BC3D5AFE"
        app:layout_constraintBottom_toTopOf="@+id/centro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/nombre"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="a"
            android:textAlignment="center"
            android:textSize="34sp" />

        <ImageView
            android:id="@+id/avatar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:src="@drawable/avatarcuatro" />
    </LinearLayout>

    <ImageView
        android:id="@+id/mapa"
        android:layout_width="153dp"
        android:layout_height="136dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="231dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="160dp"
        android:src="@drawable/circulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageninsti"
        app:layout_constraintVertical_bias="0.967" />

    <ImageView
        android:id="@+id/mapa2"
        android:layout_width="152dp"
        android:layout_height="147dp"
        android:layout_marginStart="157dp"
        android:layout_marginTop="231dp"
        android:layout_marginEnd="157dp"
        android:layout_marginBottom="160dp"
        android:src="@drawable/senderismo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageninsti"
        app:layout_constraintVertical_bias="0.935" />

    <TextView
        android:id="@+id/RUTA"
        android:layout_width="216dp"
        android:layout_height="42dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="122dp"
        android:layout_marginBottom="89dp"
        android:text="SENDERISMO"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapa2"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>